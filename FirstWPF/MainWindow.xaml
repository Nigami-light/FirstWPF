<Window x:Class="FirstWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FirstWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Background="#9BC58B" Width="155">
            <Button x:Name="btn_1" Content="1 Задание" Margin="10" Background="#476D39" Width="105" Foreground="#FFEAEAEA" Click="btn_1_Click"/>
            <Button x:Name="btn_2" Content="2 Задание" Margin="10" Background="#476D39" Width="105" Foreground="#FFEAEAEA" Click="btn_2_Click"/>
        </StackPanel>

        <Frame x:Name="myframe" Width="645" HorizontalAlignment="Right" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
